    <!-- https://rarebib.pkudh.org/ontology/CABC#banKou -->

    <owl:DatatypeProperty rdf:about="https://rarebib.pkudh.org/ontology/CABC#banKou">
        <rdfs:subPropertyOf rdf:resource="https://rarebib.pkudh.org/ontology/CABC#layout"/>
        <rdfs:domain rdf:resource="https://rarebib.pkudh.org/ontology/CABC#Edition"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="zh">指书籍装订成册后开合一端的侧面，有“白口”、“黑口”等款式。</rdfs:comment>
        <skos:altLabel xml:lang="zh">书口</skos:altLabel>
        <skos:altLabel xml:lang="zh">口</skos:altLabel>
        <skos:altLabel xml:lang="zh">页口</skos:altLabel>
        <skos:prefLabel xml:lang="zh">版口</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- https://rarebib.pkudh.org/ontology/CABC#banKuang -->

    <owl:DatatypeProperty rdf:about="https://rarebib.pkudh.org/ontology/CABC#banKuang">
        <rdfs:subPropertyOf rdf:resource="https://rarebib.pkudh.org/ontology/CABC#layout"/>
        <rdfs:domain rdf:resource="https://rarebib.pkudh.org/ontology/CABC#Edition"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="zh">指书籍书页四周的界限。主要形式为边线栏。四周单线者称“四周单边”，四周双线者称“四周双边”，上下单线而左右双线者称“左右双边”。为求艺术美观，也有用图案、花样组成的板框，称为“花边”。</rdfs:comment>
        <skos:altLabel xml:lang="zh">框廓</skos:altLabel>
        <skos:altLabel xml:lang="zh">边栏</skos:altLabel>
        <skos:prefLabel xml:lang="zh">版框</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- https://rarebib.pkudh.org/ontology/CABC#banXin -->

    <owl:DatatypeProperty rdf:about="https://rarebib.pkudh.org/ontology/CABC#banXin">
        <rdfs:subPropertyOf rdf:resource="https://rarebib.pkudh.org/ontology/CABC#layout"/>
        <rdfs:domain rdf:resource="https://rarebib.pkudh.org/ontology/CABC#Edition"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="zh">指古籍书页两半页之间没有正文的一行。为折装帧整齐，版心多刻有鱼纹、口线等，为便检索，也常记有书名、卷数、页码、每卷小题、刻工姓名等文字。</rdfs:comment>
        <skos:altLabel xml:lang="zh">心</skos:altLabel>
        <skos:altLabel xml:lang="zh">页心</skos:altLabel>
        <skos:prefLabel xml:lang="zh">版心</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- https://rarebib.pkudh.org/ontology/CABC#hangGe -->

    <owl:DatatypeProperty rdf:about="https://rarebib.pkudh.org/ontology/CABC#hangGe">
        <rdfs:subPropertyOf rdf:resource="https://rarebib.pkudh.org/ontology/CABC#layout"/>
        <rdfs:domain rdf:resource="https://rarebib.pkudh.org/ontology/CABC#Edition"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="zh">指书中正文的行数和字数。同一种书，不同时期由不同刻书家所刻印的行款往往不同。因此，行款是区别版本的一种依据。</rdfs:comment>
        <skos:altLabel xml:lang="zh">行款</skos:altLabel>
        <skos:prefLabel xml:lang="zh">行格</skos:prefLabel>
    </owl:DatatypeProperty>
    


